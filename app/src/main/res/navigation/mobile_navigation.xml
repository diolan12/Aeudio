<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.syscode.aeudio.ui.main.music.MusicFragment"
        android:label="@string/title_musics"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.syscode.aeudio.ui.main.playlist.DashboardFragment"
        android:label="@string/title_playlists"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.syscode.aeudio.ui.main.account.AccountFragment"
        android:label="@string/title_account"
        tools:layout="@layout/fragment_notifications" />
</navigation>